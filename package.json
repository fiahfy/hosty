{
  "name": "hosty",
  "productName": "Hosty",
  "version": "0.0.1",
  "description": "Application for hosts management",
  "keywords": [
    "Electron",
    "hosts"
  ],
  "author": {
    "name": "fiahfy",
    "email": "d.fiahfy@gmail.com",
    "url": "http://fiahfy.blogspot.com/"
  },
  "homepage": "http://fiahfy.blogspot.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/fiahfy/hosty"
  },
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "npm run watch & electron main.js",
    "start:hot": "npm run hot:renderer & npm run watch:main & HOT=1 electron main.js",
    "hot:renderer": "webpack-dev-server --config webpack/renderer.hot.babel.js",
    "watch": "npm run build -- -w",
    "watch:renderer": "npm run build:renderer -- -w",
    "watch:main": "npm run build:main -- -w",
    "build": "webpack --config webpack/all.babel.js",
    "build:renderer": "webpack --config webpack/renderer.babel.js",
    "build:main": "webpack --config webpack/main.babel.js",
    "package": "npm run build; electron-packager . --platform=darwin,win32 --arch=x64 --overwrite"
  },
  "dependencies": {
    "babel-loader": "6.2.4",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "electron-prebuilt": "1.2.1",
    "is-electron-renderer": "2.0.0",
    "material-ui": "0.15.0",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-helmet": "3.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.1",
    "react-router-redux": "4.0.4",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.5.2",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-logger": "2.6.1",
    "redux-persist": "3.2.2",
    "redux-thunk": "2.1.0",
    "runas": "3.1.1",
    "validator": "5.3.0",
    "webpack": "1.13.1",
    "webpack-target-electron-renderer": "0.4.0"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "1.1.1",
    "webpack-dev-server": "1.14.1"
  }
}
