{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "npm run watch & electron main.js",
    "start:hot": "npm run hot:renderer & npm run watch:main & HOT=1 electron main.js",
    "hot:renderer": "webpack-dev-server --config webpack/renderer.hot.babel.js",
    "watch": "npm run build -- -w",
    "watch:renderer": "npm run build:renderer -- -w",
    "watch:main": "npm run build:main -- -w",
    "build": "webpack --config webpack/all.babel.js",
    "build:renderer": "webpack --config webpack/renderer.babel.js",
    "build:main": "webpack --config webpack/main.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom/electron-quick-start.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/atom/electron-quick-start/issues"
  },
  "homepage": "https://github.com/atom/electron-quick-start#readme",
  "dependencies": {
    "babel-loader": "6.2.4",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "electron-json-storage": "2.0.0",
    "electron-prebuilt": "0.37.4",
    "electron-sudo": "3.0.4",
    "material-ui": "0.15.0-alpha.2",
    "react": "0.14.8",
    "react-dom": "0.14.8",
    "react-helmet": "3.0.1",
    "react-redux": "4.4.1",
    "react-router": "2.0.1",
    "react-router-redux": "3.0.0",
    "react-tap-event-plugin": "0.2.2",
    "redux": "3.3.1",
    "redux-async-connect": "1.0.0-rc4",
    "redux-devtools": "3.2.0",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.9",
    "redux-localstorage": "1.0.0-rc4",
    "redux-localstorage-filter": "0.1.1",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.0.1",
    "webpack": "1.12.14",
    "webpack-target-electron-renderer": "0.4.0"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "1.1.1",
    "webpack-dev-server": "1.14.0"
  }
}
